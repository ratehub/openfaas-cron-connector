{
  "name": "openfaas-cron-connector",
  "version": "2.1.4",
  "description": "Adds cron functionality to OpenFaaS",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "start": "node ./src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ratehub/openfaas-cron-connector.git"
  },
  "author": "Ratehub.ca",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ratehub/openfaas-cron-connector/issues"
  },
  "homepage": "https://github.com/ratehub/openfaas-cron-connector#readme",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "branches": [
      "master"
    ],
    "publish": [
      "@semantic-release/github"
    ],
    "prepare": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "newrelic": "^10.3.1",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "winston": "^3.3.3"
  }
}
